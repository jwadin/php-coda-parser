{
	"name": "jwadin/php-coda-parser",
	"description": "PHP parser for Belgian CODA banking files",
	"keywords": ["coda", "parsing", "bank"],
	"homepage": "https://github.com/wimverstuyf/php-coda-parser",
	"type": "library",
	"license": "GPL-2.0",
	"authors": [
		{ "name": "Wim Verstuyf", "email": "wim.verstuyf@codelicious.be" }
	],
	"require": {	
		"php": ">=8.2.0",
		"ext-mbstring": "*",
		"ext-ctype": "*"
	},
	"require-dev": {
		"phpunit/phpunit": "9.*",
		"friendsofphp/php-cs-fixer": "^3.51"
	},	
	"autoload": {
		"psr-4":{
			"Codelicious\\Coda\\": ["src/"]
		},
		"files": ["src/Helpers.php"]
	}
}
